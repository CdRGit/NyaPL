platform {
	[simulator] {
		fn [stdio] write_num(value: i32): () {
			unsafe [stdio] {
				return intrinsic:write_num(value);
			}
		}
	}
}

fn [stdio] main(): i32 {
	let mut a = 5;
	let (b, c, d) = (a, 5, write_num);
	write_num(a);
	a = 0;
	write_num(a);
	while a != 100 {
		write_num(a * b);
		a = a + 1;
	}
	return 0;
}
