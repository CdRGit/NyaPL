fn main(): i32 {
	return apply_tuple((12, fib_iter));
}

fn fib_iter(idx: i32): i32 {
	let mut (a, b) = (0, 1);
	let mut i = 0;
	while (i != idx) {
		(a, b) = (b, a + b);
		i = i + 1;
	}
	return a;
}

fn apply_tuple(tup: (i32, fn (i32): i32)): i32 {
	let (val, function) = tup;
	return apply(val, function);
}

fn apply(val: i32, function: fn (i32): i32): i32 {
	return function(val);
}
