fn main(): i32 {
	return big_fat_tuple_function((add_two, add_two, add_two, add_two, add_two), 0);
}

fn add_two(val: i32): i32 {
	return val + 2;
}

typedef func = fn (i32): i32;

fn big_fat_tuple_function(lot_of_functions: (func, func, func, func, func), in_val: i32): i32 {
	let (a, b, c, d, e) = lot_of_functions;
	return a(b(c(d(e(in_val)))));
}
