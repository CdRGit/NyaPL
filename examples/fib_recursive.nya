platform {
	[simulator] {
		fn [stdio] write_num(value: i32): () {
			unsafe [stdio] {
				intrinsic:write_num(value);
			}
			return ();
		}
	}
}

fn fib(idx: i32): i32 {
	if idx == 0 { return 1; }
	if idx == 1 { return 1; }

	return fib(idx - 1) + fib(idx - 2);
}

fn [stdio] main(): i32 {
	fn nested(): i32 {
		return 30;
	}
	let mut a = 0;
	while (a != nested()) {
		write_num(fib_iter(a));
		a = a + 1;
	}
	return 0;
}
